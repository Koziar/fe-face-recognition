{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:prettier/recommended"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@babel/preset-react"]
        }
    },
    "plugins": ["prettier"],
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}
